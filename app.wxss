/**app.wxss**/
.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
} 

.pull-right{
	float:right;
}
.pull-left{
	float:left;
}
.clearfix:after{
	content: '';
	display:block;
	height:0;
	visibility: hidden;
	clear:both;
}
